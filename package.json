{
    "name": "antlr-ng",
    "version": "0.1.0",
    "description": "Next generation ANTLR Tool",
    "keywords": [
        "Typescript",
        "ANTLR"
    ],
    "type": "module",
    "author": "Mike Lischke",
    "license": "MIT",
    "dependencies": {
        "antlr4ng": "2.0.8",
        "fast-printf": "1.6.9",
        "stringtemplate4ts": "1.0.1"
    },
    "devDependencies": {
        "@types/jest": "29.5.11",
        "@types/node": "20.11.5",
        "@types/unicode-properties": "1.3.2",
        "@typescript-eslint/eslint-plugin": "6.19.0",
        "@typescript-eslint/eslint-plugin-tslint": "6.19.0",
        "@typescript-eslint/parser": "6.19.0",
        "eslint": "8.56.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsdoc": "48.0.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "jest": "29.7.0",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.2",
        "typescript": "5.3.3",
        "unicode-properties": "1.4.1"
    },
    "scripts": {
        "build": "npm run generate-test-parsers && tsc",
        "lint": "eslint \"./runtime/**/*.ts\"",
        "lint:fix": "eslint \"./runtime/**/*.ts\" --fix",
        "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --no-coverage",
        "generate-test-parsers": "./build/generate-test-parsers.sh",
        "time-lexer-speed": "node --no-warnings --experimental-vm-modules --loader ts-node/esm runtime-testsuite/src/tests/api/perf/TimeLexerSpeed.ts"
    }
}
